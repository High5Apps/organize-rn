{
  "name": "Organize",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "./scripts/run-android && npm run tether:android",
    "android:device": "./scripts/run-android --device && npm run tether:android",
    "android:device:release": "./scripts/run-android --device --release",
    "check-dependencies": "rnx-align-deps",
    "deeplink:android": "adb shell am start -W -a android.intent.action.VIEW -d organize://$npm_config_path com.organize",
    "deeplink:ios": "xcrun simctl openurl booted organize://$npm_config_path",
    "fix-dependencies": "rnx-align-deps --write",
    "ios": "react-native run-ios --simulator \"iPhone SE (3rd generation)\"",
    "ios:device": "react-native run-ios --device",
    "ios:device:release": "npm run ios:device -- --mode=Release",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "test": "npm run check-dependencies && npm run type-check && npm run lint && jest",
    "tether:android": "adb reverse tcp:8080 tcp:8080",
    "type-check": "tsc"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/datetimepicker": "^7.6.2",
    "@react-native-segmented-control/segmented-control": "^2.4.2",
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/elements": "^1.3.18",
    "@react-navigation/material-top-tabs": "^6.6.3",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "buffer": "^6.0.3",
    "crypto-js": "^4.1.1",
    "react": "18.2.0",
    "react-fast-compare": "^3.2.2",
    "react-native": "^0.73.0",
    "react-native-get-random-values": "^1.8.0",
    "react-native-hyperlink": "^0.0.22",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-pager-view": "^6.2.1",
    "react-native-qrcode-svg": "^6.3.0",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.28.0",
    "react-native-svg": "^14.0.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-vis-network": "^2.0.0",
    "react-native-vision-camera": "^3.9.2",
    "react-native-webview": "^13.6.1",
    "use-debounce": "^10.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.0",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "^0.73.0",
    "@react-native/typescript-config": "0.73.1",
    "@rnx-kit/align-deps": "^2.4.2",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.0",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^18.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^29.2.1",
    "prettier": "2.8.8",
    "react-native-dotenv": "^3.4.9",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.73"
      ],
      "capabilities": [
        "babel-preset-react-native",
        "core",
        "core-android",
        "core-ios",
        "datetime-picker",
        "jest",
        "navigation/native",
        "react",
        "react-test-renderer",
        "safe-area",
        "screens",
        "storage",
        "svg",
        "webview"
      ]
    }
  }
}
