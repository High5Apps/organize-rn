{
  "name": "Organize",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "./scripts/run-android && npm run tether:android",
    "android:device": "./scripts/run-android --device && npm run tether:android",
    "android:device:release": "./scripts/run-android --device --release",
    "check-dependencies": "rnx-align-deps",
    "deeplink:android": "adb shell am start -W -a android.intent.action.VIEW -d https://getorganize.app/$npm_config_path app.getorganize.organize",
    "deeplink:ios": "xcrun simctl openurl booted https://getorganize.app/$npm_config_path",
    "fix-dependencies": "rnx-align-deps --write",
    "ios": "react-native run-ios --simulator \"iPhone SE (3rd generation)\"",
    "ios:device": "react-native run-ios --device",
    "ios:device:release": "npm run ios:device -- --mode=Release",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package",
    "start": "react-native start",
    "test": "npm run check-dependencies && npm run type-check && npm run lint && jest",
    "tether:android": "adb reverse tcp:8080 tcp:8080",
    "type-check": "tsc"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-community/datetimepicker": "^8.0.0",
    "@react-native-segmented-control/segmented-control": "^2.4.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/elements": "^2.2.5",
    "@react-navigation/material-top-tabs": "^7.1.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "buffer": "^6.0.3",
    "react": "18.3.1",
    "react-fast-compare": "^3.2.2",
    "react-native": "^0.77.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-hyperlink": "^0.0.22",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-pager-view": "^6.7.0",
    "react-native-qrcode-svg": "^6.3.0",
    "react-native-reanimated": "^3.16.7",
    "react-native-safe-area-context": "^5.1.0",
    "react-native-screens": "^4.6.0",
    "react-native-svg": "^15.11.1",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-vis-network": "^2.0.0",
    "react-native-vision-camera": "^4.6.3",
    "react-native-webview": "^13.13.1",
    "use-debounce": "^10.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@kesills/eslint-config-airbnb-typescript": "^20.0.0",
    "@react-native-community/cli": "^15.0.1",
    "@react-native-community/cli-platform-android": "^15.0.1",
    "@react-native-community/cli-platform-ios": "^15.0.1",
    "@react-native/babel-preset": "^0.77.0",
    "@react-native/metro-config": "^0.77.0",
    "@react-native/typescript-config": "0.77.0",
    "@rnx-kit/align-deps": "^3.0.3",
    "@types/jest": "^29.5.13",
    "@types/node": "^18.11.0",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^18.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "jest": "^29.2.1",
    "patch-package": "^8.0.0",
    "prettier": "2.8.8",
    "react-native-dotenv": "^3.4.9",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.77"
      ],
      "capabilities": [
        "babel-preset-react-native",
        "core",
        "core-android",
        "core-ios",
        "datetime-picker",
        "jest",
        "navigation/native",
        "react",
        "react-test-renderer",
        "safe-area",
        "screens",
        "storage",
        "svg",
        "webview"
      ],
      "presets": [
        "microsoft/react-native",
        "./rnx-kit-overrides.js"
      ]
    }
  }
}
