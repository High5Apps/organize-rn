{
  "name": "organize",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:device": "npm run android -- --deviceId=$(adb devices | tail -n +2 | grep -m 1 -v emulator | cut -f1)",
    "android:device:release": "npm run android:device -- --variant=release",
    "ios": "react-native run-ios --simulator \"iPhone 8 (15.5)\"",
    "ios:device": "react-native run-ios --device",
    "ios:device:release": "npm run ios:device -- --configuration=Release",
    "deeplink:android": "adb shell am start -W -a android.intent.action.VIEW -d organize://$npm_config_path com.organize",
    "deeplink:ios": "xcrun simctl openurl booted organize://$npm_config_path",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "test": "npm run type-check && npm run lint && jest",
    "type-check": "tsc"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.6",
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "buffer": "^6.0.3",
    "crypto-js": "^4.1.1",
    "react": "17.0.2",
    "react-native": "0.67.3",
    "react-native-gesture-handler": "^2.3.1",
    "react-native-get-random-values": "^1.8.0",
    "react-native-keyboard-accessory": "^0.1.16",
    "react-native-qrcode-svg": "^6.1.2",
    "react-native-reanimated": "^2.4.1",
    "react-native-rsa-native": "^2.0.5",
    "react-native-safe-area-context": "^4.1.3",
    "react-native-screens": "^3.13.1",
    "react-native-svg": "^12.3.0",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-vector-icons": "^9.1.0",
    "react-native-vision-camera": "^2.13.1",
    "uuid": "^8.3.2",
    "vision-camera-code-scanner": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.15",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^17.0.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  }
}
