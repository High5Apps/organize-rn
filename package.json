{
  "name": "Organize",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:device": "npm run android -- --deviceId=$(adb devices | tail -n +2 | grep -m 1 -v emulator | cut -f1)",
    "android:device:release": "npm run android:device -- --mode=release",
    "check-dependencies": "rnx-align-deps",
    "deeplink:android": "adb shell am start -W -a android.intent.action.VIEW -d organize://$npm_config_path com.organize",
    "deeplink:ios": "xcrun simctl openurl booted organize://$npm_config_path",
    "fix-dependencies": "rnx-align-deps --write",
    "ios": "react-native run-ios --simulator \"iPhone 8\"",
    "ios:device": "react-native run-ios --device",
    "ios:device:release": "npm run ios:device -- --mode=Release",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "test": "npm run check-dependencies && npm run type-check && npm run lint && jest",
    "tether:android": "adb reverse tcp:3000 tcp:3000",
    "type-check": "tsc"
  },
  "dependencies": {
    "@bigbee.dev/react-native-measure-text-size": "^1.1.0",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-masked-view/masked-view": "^0.2.7",
    "@react-native-segmented-control/segmented-control": "^2.4.2",
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/elements": "^1.3.18",
    "@react-navigation/material-top-tabs": "^6.6.3",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "@react-navigation/stack": "^6.2.0",
    "buffer": "^6.0.3",
    "crypto-js": "^4.1.1",
    "react": "18.2.0",
    "react-native": "^0.71.0",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-get-random-values": "^1.8.0",
    "react-native-keyboard-accessory": "^0.1.16",
    "react-native-pager-view": "^6.2.1",
    "react-native-qrcode-svg": "^6.2.0",
    "react-native-reanimated": "^2.14.1",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.19.0",
    "react-native-svg": "^13.7.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-vector-icons": "^9.1.0",
    "react-native-vis-network": "^0.9.0",
    "react-native-vision-camera": "^2.15.4",
    "react-native-webview": "^12.0.2",
    "uuid": "^8.3.2",
    "vision-camera-code-scanner": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@rnx-kit/align-deps": "^2.2.1",
    "@tsconfig/react-native": "^2.0.2",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.0",
    "@types/react": "^18.0.24",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^18.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.71"
      ],
      "capabilities": [
        "animation",
        "babel-preset-react-native",
        "core",
        "core-android",
        "core-ios",
        "gestures",
        "jest",
        "masked-view",
        "navigation/native",
        "navigation/stack",
        "react",
        "react-test-renderer",
        "safe-area",
        "screens",
        "storage",
        "svg"
      ]
    }
  }
}
